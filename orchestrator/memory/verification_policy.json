{
  "version": 1,
  "test_requirements": {
    "min_coverage": 80,
    "must_pass_before_review": true
  },
  "report_rules": {
    "apply_to": ["REVIEW", "TEST", "FINISH_REVIEW"],
    "require_verdict": true,
    "verdict_prefix": "结论：",
    "verdict_allowed": ["PASS", "FAIL", "BLOCKED"],
    "blocker_prefix": "阻塞：",
    "blocker_clear_value": "无"
  }
}
